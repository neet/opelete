  "license": "MIT",
  "scripts": {
    "test": "npm run test:lint && npm run test:jest",
    "test:lint": "eslint -c .eslintrc.yml --ext=js src/",
    "test:jest": "cross-env NODE_ENV=test jest --coverage --config jest.config.json",
    "build:prod": "webpack --mode=production --no-progress --hide-modules",
    "build:dev": "webpack --mode=development --no-progress --hide-modules",
    "start": "npm run build:dev && web-ext run -s dist/",
    "postinstall": "npm rebuild node-sass"
  },
