{
  "projects": [
    "<rootDir>/src/opelete"
  ],
  "testPathIgnorePatterns": [
    "<rootDir>/node_modules/"
  ],
  "setupFiles": [
    "<rootDir>/src/opelete/test_setup.js"
  ],
  "collectCoverageFrom": [
    "src/opelete/**/*.js"
  ],
  "coverageDirectory": "<rootDir>/coverage",
  "transform": {
    "^.+\\.js$": "babel-jest"
  }
}
